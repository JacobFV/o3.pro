{
  "name": "o3.pro",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "with-env": "env-cmd -f .env.${NODE_ENV}",
    "build": "bun run with-env -- next build",
    "check": "bun run with-env -- next lint && tsc --noEmit",
    "db:generate": "bun run with-env -- drizzle-kit generate",
    "db:migrate": "bun run with-env -- drizzle-kit migrate",
    "db:push": "bun run with-env -- drizzle-kit push",
    "db:studio": "bun run with-env -- drizzle-kit studio",
    "dev:db:spawn": "bun run with-env -- ./start-database.sh",
    "dev:db:kill": "bun run with-env -- docker stop $DB_CONTAINER_NAME",
    "dev": "bun run with-env -- next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "bun run next build && bun run next start",
    "start": "bun run next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.7.2",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.50.0",
    "@trpc/client": "^11.0.0-rc.446",
    "@trpc/react-query": "^11.0.0-rc.446",
    "@trpc/server": "^11.0.0-rc.446",
    "drizzle-orm": "^0.33.0",
    "geist": "^1.3.0",
    "next": "^15.0.1",
    "next-auth": "5.0.0-beta.25",
    "nodemailer": "^6.9.16",
    "postgres": "^3.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "drizzle-kit": "^0.24.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.1",
    "eslint-plugin-drizzle": "^0.2.3",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.5.3"
  },
  "ct3aMetadata": {
    "initVersion": "7.38.1"
  }
}